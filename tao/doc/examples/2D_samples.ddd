page "polygon",
    color "tao_orange" 
    poly_x -> 0 
    poly_y -> 0 
    poly_w -> 200 
    poly_h -> 200 
    poly nb, dx, dy ->
        locally
            translate dx, dy, 0
            polygon poly_x, poly_y, poly_w, poly_h, nb
            axes 
            line_color "black" 
            width_and_height poly_x, poly_y, poly_w, poly_h
    poly  5, -350,  175
    poly  6,    0,  175
    poly  7,  350,  175
    poly 10, -350, -175
    poly 12,    0, -175
    poly 15,  350, -175

page "polygram",
    color "tao_orange" 
    poly_x -> 0 
    poly_y -> 0 
    poly_w -> 200
    poly_h -> 200 
    polyg nb, i, dx, dy ->
        locally
            translate dx, dy, 0
            polygram poly_x, poly_y, poly_w, poly_h, nb, i
            axes 
            line_color "black" 
            width_and_height poly_x, poly_y, poly_w, poly_h
    polyg  5, 2, -350,  175
    polyg  6, 2,    0,  175
    polyg  7, 2,  350,  175
    polyg 10, 3, -350, -175
    polyg 12, 4,    0, -175
    polyg 15, 4,  350, -175
