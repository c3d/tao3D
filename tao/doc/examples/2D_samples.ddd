page "ellipse_arc", 
    color "tao_orange" 
    //line_color "tao_orange"
    ellipsea_x -> 0 
    ellipsea_y -> 0 
    ellipsea_w -> 200 
    ellipsea_h -> 290 
    ellipsea_start -> 20 
    ellipsea_sweep -> 135 
    ellipse_arc ellipsea_x, ellipsea_y, ellipsea_w, ellipsea_h, ellipsea_start, ellipsea_sweep 
    axes 
    line_color "black" 
    width_and_height ellipsea_x, ellipsea_y, ellipsea_w, ellipsea_h 

    // Start angle
    color "transparent" 
    path 
        move_to 61, 0 
        endpoints_style BAR, ARROWHEAD 
        quad_to 68, 17, 57, 30 

    locally 
        line_color "font" 
        color "font" 
        translate 70, 17, 0 
        text "start angle" 

    // Sweep angle
    path 
        move_to 68, 38 
        endpoints_style BAR, ARROWHEAD 
        quad_to 4, 105, -57, 41 

    locally 
        line_color "font" 
        color "font" 
        translate -80, 73, 0 
        text "sweep angle" 

page "isocel_triangle", 
    color "tao_orange" 
