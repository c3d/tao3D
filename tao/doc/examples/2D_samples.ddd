page "star", 
    color "tao_orange" 
    //line_color "tao_orange"
    star_x -> 0 
    star_y -> 0 
    star_w -> 250 
    star_h -> 200 
    star_vertex -> 8 
    star_ratio -> 50% 
    star star_x, star_y, star_w, star_h, star_vertex, star_ratio 
    axes 
    line_color "black" 
    width_and_height star_x, star_y, star_w, star_h 
    line_arrow 0, 0, ARROWHEAD, -23, -45, ARROWHEAD 
    locally 
        rotatez 58 
        line_color "transparent" 
        color "black" 
        translate -45, 0, 0 
        text "r * " 
        color "tao_yellow" 
        text "L" 
    line_color "tao_yellow" 
    line_width 1.5 

    shape 
        line_arrow 0, 0, ARROWHEAD, -89, 71, ARROWHEAD 
    locally 
        line_color "transparent" 
        color "tao_yellow" 
        rotatez -42 
        translate -60, 0, 0 
        text "L" 


